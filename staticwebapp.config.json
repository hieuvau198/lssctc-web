{
  "navigationFallback": {
    "rewrite":  "/index.html",
    "exclude": ["/assets/**", "/static/**", "**/*.{js,css,png,jpg,jpeg,gif,svg,ico,json,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ". js": "text/javascript",
    ".mjs": "text/javascript",
    ".json": "application/json",
    ".css": "text/css"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  }
}